{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText_with_JSON",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "parse1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          coupon_id as string,",
				"          coupon_code as string,",
				"          coupon_name as string,",
				"          status as string,",
				"          coupon_class as string,",
				"          coupon_type as string,",
				"          redeem_method as string,",
				"          smile_num as string,",
				"          image_url as string,",
				"          description as string,",
				"          description_for_redeem as string,",
				"          sanrio_event_id as string,",
				"          number_of_people_required as string,",
				"          number_of_people_limit as string,",
				"          stock_unit as string,",
				"          authority as string,",
				"          exchange_count_type as string,",
				"          exchange_limit as string,",
				"          exchange_criteria as string,",
				"          redeem_criteria as string,",
				"          display_used_coupon as string,",
				"          start_timestamp as string,",
				"          end_timestamp as string,",
				"          expiration_date as string,",
				"          expiration_duration as string,",
				"          created as string,",
				"          modified as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"select1 parse(type = type ? string,",
				"     format: 'json',",
				"     documentForm: 'documentPerLine') ~> parse1",
				"source1 select(mapColumn(",
				"          exchange_criteria",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}